# SimWorld Career Development Platform - Frontend Environment Variables
# Copy this file to .env and update the values as needed

# API Configuration - Backend server URL
REACT_APP_API_URL=http://localhost:8000/api

# App Configuration
REACT_APP_NAME=SimWorld - AI Career Development Platform
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_MOCK_DATA=true

# Google AI Configuration
REACT_APP_GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# Optional: Analytics or other services
# REACT_APP_GA_TRACKING_ID=your-google-analytics-id
# REACT_APP_SENTRY_DSN=your-sentry-dsn
